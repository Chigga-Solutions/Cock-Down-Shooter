{
  "extends": [
    "next/core-web-vitals", 
    "next/typescript", 
    "prettier", 
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "plugins": ["import"],
  "rules": {
    "import/no-unused-modules": [
      1,
      {
        "unusedExports": false,
        "missingExports": true
      }
    ]
  }
}
